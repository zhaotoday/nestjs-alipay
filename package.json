{
  "name": "nestjs-alipay",
  "version": "0.0.3",
  "description": "NestJS 支付宝支付模块",
  "keywords": [
    "nestjs",
    "alipay",
    "payment",
    "typescript"
  ],
  "author": "zhaojintian",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhaotoday/nestjs-alipay"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsc -b -v",
    "clean": "rimraf dist",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@nestjs-repo/eslint-config": "workspace:*",
    "@nestjs-repo/typescript-config": "workspace:*",
    "@types/express": "^5.0.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.7",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "@nestjs/swagger": "^11.2.1"
  },
  "dependencies": {
    "alipay-sdk": "^4.14.0"
  }
}
